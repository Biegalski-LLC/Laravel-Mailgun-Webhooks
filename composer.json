{
    "name": "biegalski-llc/laravel-mailgun-webhooks",
    "description": "This package allows you to quick and easily integrate your Laravel application with Mailgun Webhooks thus allowing you to track the outgoing email status for each individual user. There is also an optional feature to receive alerts for specific mailgun events such as spam complaints and sending failuresgit .",
    "keywords": ["laravel", "mailgun", "webhooks", "php", "email delivery status"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Biegalski",
            "email": "packages@biegalski-llc.com",
            "homepage": "https://biegal.ski/"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Biegalski\\LaravelMailgunWebhooks\\LaravelMailgunWebhooksServiceProvider"
            ]
        }
    },
    "require": {
        "php" : "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Biegalski\\LaravelMailgunWebhooks\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Biegalski\\LaravelMailgunWebhooks\\tests\\": "tests"
        }
    }
}
